@model IEnumerable<StackOverflow.Models.ApplicationUser>

@{
    ViewData["Title"] = "Questions";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="well">
    <h2>Users of StackOverflow</h2>
    <hr/>

    @if (Model.Any())
    {
        <ul>
            @foreach (var user in Model)
            {
                <li>@Html.DisplayFor(x => user.UserName)</li>
            }
        </ul>
    }
    else
    {
        <p>No Users!</p>
    }

    @Html.ActionLink("Back", "Index", "Home")
</div>